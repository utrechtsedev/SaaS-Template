      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.23",
    "publint": "^0.3.2",
    "svelte": "^5.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.2",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@sveltejs/adapter-vercel": "^5.6.1"
  }
